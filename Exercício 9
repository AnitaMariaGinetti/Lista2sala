num1 = int(input("Entre com o primeiro número inteiro: "))
num2 = int(input("Entre com o segundo número inteiro: "))

if (num1 % 4 == 0) or (num1 % 5 == 0):
    print(f"O número {num1} é divisível por 4 ou por 5.")

if (num2 % 4 == 0) or (num2 % 5 == 0):
    print(f"O número {num2} é divisível por 4 ou por 5.")
    
else:
	print("Nenhum dos números é divisível por 4 ou por 5.")
